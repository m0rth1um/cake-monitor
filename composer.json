{
    "name": "scherersoftware/cake-monitor",
    "description": "A simple config based monitoring plugin for CakePHP 3",
    "license": "MIT",
    "type": "cakephp-plugin",
    "extra": {
        "installer-name": "Monitor"
    },
    "require": {
        "cakephp/plugin-installer": "*",
        "sentry/sentry": "1.6.*",
        "php": ">=7.0"
    },
    "require-dev": {
        "cakephp/cakephp": "~3.0"
    },
    "autoload": {
        "psr-4": {
            "Monitor\\": "src"
        }
    }
}